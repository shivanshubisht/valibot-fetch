{
  "name": "valibot-fetch",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint & manypkg check",
    "test": "turbo test",
    "typecheck": "turbo  typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "changeset version && changeset publish"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@manypkg/cli": "^0.21.0",
    "@types/node": "20.7.2",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.6.10"
}
